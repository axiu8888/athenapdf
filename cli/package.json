{
  "name": "athenapdf",
  "version": "2.16.0",
  "description": "A simple CLI tool to convert HTML to PDF from a local file or a URL to a web page using Electron (Chromium).",
  "keywords": [
    "electron",
    "chrome",
    "cli",
    "html",
    "pdf",
    "converter",
    "generate"
  ],
  "homepage": "https://www.athenapdf.com/",
  "bugs": {
    "url": "https://github.com/arachnys/athenapdf/issues"
  },
  "license": "MIT",
  "author": "Ian Lai <ian@arachnys.com>",
  "main": "athenapdf.js",
  "bin": "./bin/athenapdf",
  "repository": {
    "type": "git",
    "url": "git://github.com/arachnys/athenapdf.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build",
    "build": "npm run clean && npm run build:prepare && npm run build:linux",
    "build:prepare": "mkdir -p ./build && cp -r src/ ./build/artifacts/ && cp package.json ./build/artifacts/ && cd ./build/artifacts/ && npm i --production",
    "build:linux": "electron-packager build/artifacts/ athenapdf --platform=linux --arch=x64 --version=3.0.5 --out=build/ --overwrite --asar=true",
    "build:win_x32": "electron-packager build/artifacts/ athenapdf --win32 --arch=ia32 --out ./build/ --overwrite --app-version=1.0.0 --icon=./favicon.ico --asar",
    "build:win_x64": "electron-packager build/artifacts/ athenapdf --win32 --arch=x64 --out ./build/ --overwrite --app-version=1.0.0 --icon=./favicon.ico --asar"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "rw": "^1.3.3"
  },
  "devDependencies": {
    "electron-packager": "^17.1.2",
    "electron": "^26.2.0"
  }
}
